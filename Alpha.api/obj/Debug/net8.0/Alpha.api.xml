        <member name="M:Alpha.api.Controllers.ProductsController.GetProducts(System.Int32,System.Int32,System.String)">
            <summary>
            Consulta de Produtos
            </summary>
            <param name="page"></param>
            <param name="pageSize">Quantidade de produtos por página. O padrão é 10.</param>
            <param name="sortBy">Campo pelo qual os produtos serão ordenados. Deixe vazio para ordenação padrão.</param>
            <returns>Uma lista paginada de produtos.</returns>
            <remarks>
            Exemplo de uso:
            <code>
            GET /products?page=2&amp;pageSize=20&amp;sortBy=nome
            </code>
            </remarks>
            <response code="200">Uma listagem com os produtos</response>
        </member>
        <member name="M:Alpha.api.Controllers.ProductsController.CreateProduct(Alpha.api.Models.PostProductVM)">
             <summary>
             Salvar um produto
             </summary>
             <remarks>
             Exemplo:
            
                 POST /products
                 {
                     name: 'Logo',
                     price: 0,
                     barCode: '',
                     imageBase64: ''
                 }
             </remarks>
        </member>
        <member name="M:Alpha.api.Controllers.ProductsController.UpdateProduct(Alpha.api.Models.PutProductVM)">
             <summary>
             Editar um produto
             </summary>
             <remarks>
             Exemplo:
            
                 PUT /products
                 {
                     'id': '1'
                     name: '',
                     price: 0,
                     barCode: '',
                     imageBase64: ''
                 }
            
             </remarks>
        </member>
